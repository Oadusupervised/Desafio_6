<h1>realTimeMessages</h1>
<form>
<label for="user">user</label><input id="inputUser" name="user" type="text">
<label for="message">message</label><input id="inputMessages" name="message" type="text">
<button type="submit" id="btnEnviar3">enviar</button>
</form>
<h3>mensajesActuales</h3>
<div id="listadoMensajes">
    {{#if hayMensajes}}
    <ul>
        {{#each mensajes}}
        <li>user: {{this.user}} | message: {{this.message}}
        </li>
        {{/each}}
    </ul>
    {{else}}
    <p>no hay mensajes para mostrar</p>
    {{/if}}
</div>

